{
  "name": "plasmo-extension",
  "displayName": "Plasmo extension",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "richard",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "google-auth-library": "8.1.0",
    "next": "12.2.1",
    "plasmo": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/rps": "latest",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chrome": "0.0.193",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "identity",
      "identity.email"
    ],
    "key": "$CRX_PUBLIC_KEY",
    "oauth2": {
      "client_id": "$OAUTH_CLIENT_ID",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    }
  }
}
